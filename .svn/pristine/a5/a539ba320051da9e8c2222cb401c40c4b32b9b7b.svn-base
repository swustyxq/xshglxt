package StuMtrManageSys.model;

public class Material {
    private Integer mtrId;

    private String mtrName;

    private Integer mtrTotalnum;

    private Integer mtrStocknum;

    private String mtrDeptname;

    private Integer mtrImportance;

    private String mtrSize;

    private String mtrColor;

    private String mtrType;

    private String mtrPostil;

    public Integer getMtrId() {
        return mtrId;
    }

    public void setMtrId(Integer mtrId) {
        this.mtrId = mtrId;
    }

    public String getMtrName() {
        return mtrName;
    }

    public void setMtrName(String mtrName) {
        this.mtrName = mtrName;
    }

    public Integer getMtrTotalnum() {
        return mtrTotalnum;
    }

    public void setMtrTotalnum(Integer mtrTotalnum) {
        this.mtrTotalnum = mtrTotalnum;
    }

    public Integer getMtrStocknum() {
        return mtrStocknum;
    }

    public void setMtrStocknum(Integer mtrStocknum) {
        this.mtrStocknum = mtrStocknum;
    }

    public String getMtrDeptname() {
        return mtrDeptname;
    }

    public void setMtrDeptname(String mtrDeptname) {
        this.mtrDeptname = mtrDeptname;
    }

    public Integer getMtrImportance() {
        return mtrImportance;
    }

    public void setMtrImportance(Integer mtrImportance) {
        this.mtrImportance = mtrImportance;
    }

    public String getMtrSize() {
        return mtrSize;
    }

    public void setMtrSize(String mtrSize) {
        this.mtrSize = mtrSize;
    }

    public String getMtrColor() {
        return mtrColor;
    }

    public void setMtrColor(String mtrColor) {
        this.mtrColor = mtrColor;
    }

    public String getMtrType() {
        return mtrType;
    }

    public void setMtrType(String mtrType) {
        this.mtrType = mtrType;
    }

    public String getMtrPostil() {
        return mtrPostil;
    }

    public void setMtrPostil(String mtrPostil) {
        this.mtrPostil = mtrPostil;
    }
}